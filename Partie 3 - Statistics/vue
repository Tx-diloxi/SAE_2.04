
DROP VIEW partie3.vue;

CREATE VIEW partie3.vue AS
SELECT
    i.prenom,
    i.code_postal,
    c.niveau_etude,
    r.moyenne,
    e.mention_bac,
FROM partie3._individu i
JOIN partie3._etudiant e ON i.ine = e.ine
JOIN partie3.resultats_annee ra ON e.code_nip = ra.code_nip

_candidat
WHERE ra.m2105 IS NOT NULL;


SELECT * FROM partie3.vue;


WbExport -file=mon_fichier_csv_de_sortie.csv
         -outputDir=.
         -type=text
         -sourceTable=partie3.vue
         -schema=partie3
         -delimiter=';'
         -header=true
         -keyColumns=prenom,moyenne,code_postal,niveau_etude,mention_bac
         ;
